te>\n\n<script>\nexport default {\n    name:'Menu',\n    methods: {\n        rolar : function(event) {\n            var elemento = event.currentTarget\n            var idAlvo = elemento.innerHTML.toLowerCase()\n            var coord = document.getElementById(idAlvo).offsetTop\n\n            if (idAlvo == 'home') {\n                if (document.getElementById('contato-form') != null) {\n                    coord -= document.getElementById('contato-form').offsetTop\n                } else {\n                    coord = 0\n                }\n            }\n            \n            window.scroll({\n                top: coord,\n                behavior: 'smooth',\n            })\n        },\n        rolarespecial: function () {\n            window.scroll({\n                top: 0,\n                behavior: 'smooth',\n            })\n        }\n    }\n}\n</script>\n\n<style scoped>\n    #navbar{\n        color: white;\n        background-color: rgba(69, 65, 255, 0.8);\n        position: fixed;\n        top: 0;\n        height: 13.4vh;\n        width: 94%;\n        padding-right: 3%;\n        padding-left: 3%;\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n    }\n\n    #logo{\n        height: 100%;\n        width: auto;\n        display: flex;\n        flex-direction: row;\n    }\n\n    #logo img{\n        height: 100%;\n    }\n\n    #logo-texto{\n        padding-left: 10%;\n        text-align: left;\n        display: flex;\n        justify-content: center;\n        flex-direction: column;\n    }\n\n    #logo-texto :nth-child(1){\n        font-size: 0.9rem;\n    }\n\n    #logo-texto :nth-child(2){\n        width: max-content;\n        font-size: 1.8rem;\n    }\n\n    #menu{\n        width: 60%;\n        font-size: 3.2vh;\n        display: flex;\n        flex-direction: row;\n        justify-content: space-between;\n    }\n\n    #menu button{\n        background-color: transparent;\n        border-color: transparent;\n        color: white;\n        cursor: pointer;\n    }\n\n    #menu a{\n        text-decoration: none;\n        color: white;\n    }\n</style>"]}]}